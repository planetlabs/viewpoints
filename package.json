{
  "name": "starter",
  "version": "1.0.0",
  "description": "A simple React static site development boilerplate",
  "author": "Trevor Blades",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevorblades/starter.git"
  },
  "scripts": {
    "start": "gulp dev",
    "dist": "gulp dist",
    "lint": "eslint src gulpfile.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "node_modules",
          "plugins": [
            "transform-runtime"
          ],
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "planet/react"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.0.16",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.9.11",
    "browserify": "^12.0.1",
    "del": "^2.2.0",
    "eslint": "^2.0.0",
    "eslint-config-planet": "^5.0.0",
    "eslint-plugin-react": "^4.2.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-less": "^3.0.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.7",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "normalize-css": "^2.3.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "lodash.chunk": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.flatten": "^4.4.0",
    "papaparse": "^4.1.2",
    "platform": "^1.3.1",
    "react-bootstrap": "^0.29.5",
    "react-bootstrap-typeahead": "^0.8.3",
    "react-progress-bar-plus": "^1.2.0"
  }
}
